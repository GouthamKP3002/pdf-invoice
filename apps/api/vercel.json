{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri",
    "BLOB_READ_WRITE_TOKEN": "@blob-read-write-token",
    "GEMINI_API_KEY": "@gemini-api-key",
    "GROQ_API_KEY": "@groq-api-key",
    "NODE_ENV": "production"
  }
}